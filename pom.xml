    <name>spring-boot-rabbitmq</name>
    <description>Demo project for Spring Boot Integrated with Rabbit MQ</description>
    <parent>
        <groupId>org.springframework.boot</groupId>
        <artifactId>spring-boot-starter-parent</artifactId>
